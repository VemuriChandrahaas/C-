#include <iostream>

class Car {
public:
    static int carCount;

    Car() {
        carCount++;
    }

    void displayCarInfo() {
        std::cout << "This is a car object." << std::endl;
    }
};

int Car::carCount = 0;

int main() {
    Car car1;
    Car car2;
    Car car3;

    std::cout << "Total cars created: " << Car::carCount << std::endl;

    return 0;
}
